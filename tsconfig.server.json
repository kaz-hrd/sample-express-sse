{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES2019",
    "module": "commonjs",
    "outDir": "./server/",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/server/*", "src/common/**/*"]
}
